package Tetris.Game {		import flash.display.MovieClip;	import Tetris.Game.Blocks.*;	import Tetris.Core.*;		public class Blocks {		private static var stage;		private static var test;		private static var y = 0;				public static function initialize(stage) {			Blocks.stage = stage;						// Spawn block			Blocks.test = new BlockYellow();			Blocks.test.x = Utils.translate(0);			Blocks.test.y = Utils.translate(0);						stage.addChild(Blocks.test);		}				public static function update() {			Blocks.y++;			Blocks.test.y = Utils.translate(Blocks.y);		}	}}