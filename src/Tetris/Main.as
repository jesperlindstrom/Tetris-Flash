package Tetris {	import flash.display.MovieClip;	import flash.events.*;	import Tetris.Game.*;	import Tetris.Game.Blocks.*;	import flash.utils.setInterval;		public class Main extends MovieClip {		public function Main() {			// Set frame rate			stage.frameRate = 30;						// Initialize			UI.initialize(stage, TextScore, TextLines, NextBlockArea);			Blocks.initialize(stage);						setInterval(Blocks.update, Config.rate);						// Handle frame update			addEventListener(Event.ENTER_FRAME, function(e) {				Blocks.render();			});		}	}}