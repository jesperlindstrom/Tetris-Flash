package Tetris {	import flash.display.MovieClip;	import flash.events.*;	import Tetris.Game.*;	import Tetris.Game.Blocks.*;		public class Main extends MovieClip {		public function Main() {			// Config			stage.frameRate = 2; // 2 = 500ms per tick						// Initialize			UI.initialize(stage);			Blocks.initialize(stage);						addEventListener(Event.ENTER_FRAME, this.update);		}				public function update(e) {			Blocks.update();		}	}}